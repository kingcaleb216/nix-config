* {
    background-color: transparent;
    text-color: #FFFFFF;
    font: "Fira Code 12";
}

window {
    background-color: rgba(0, 0, 0, 0.6);
    border: 2px;
    border-radius: 10px;
    padding: 10px;
}

inputbar {
    background-color: transparent;
    border: 1px;
    padding: 10px;
}

listview {
    background-color: transparent;
    padding: 10px;
}

element {
    padding: 5px;
    border-radius: 5px;
}

element selected {
    background-color: rgba(30, 30, 30, 1);
}

